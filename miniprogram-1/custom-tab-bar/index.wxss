/* custom-tab-bar/index.wxss */
.tabBar {
  position: fixed;
  display: flex;
  justify-content: space-between;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  background-color: #ffffff;
  border-top: 1rpx solid #E6E6E6;
  z-index: 998;
}
.tab {
  flex: 1;
  cursor: pointer;
  text-align: center;
}
.tab .icon {
  width: 88rpx;
  height: 88rpx;
  padding: 5rpx;
}